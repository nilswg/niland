{
  "name": "tsup-example",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess \"node dist/index.js -e development\"",
    "start": "node dist/index.js -e production",
    "build": "tsup ./src/index.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "tsup": "^8.0.1",
    "typescript": "5.3.3"
  }
}
