{
  "name": "ts-example",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "version": "0.0.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./index.ts",
    "build": "rm -rf dist && tsc && tsc-alias"
  },
  "devDependencies": {
    "@types/node": "20.4.9",
    "nodemon": "3.0.1",
    "ts-node": "10.9.1",
    "tsc-alias": "1.8.7",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
