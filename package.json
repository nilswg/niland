{
  "name": "tsup-jest",
  "type": "commonjs",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess \"node dist/index.js -e development\"",
    "start": "node dist/index.js -e production",
    "build": "tsup ./src/index.ts",
    "test:cjs": "jest --verbose --config ./jest.config.cjs",
    "test:esm": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --config ./jest.config.mjs",
    "test": "pnpm run test:cjs && pnpm run test:esm"
  },
  "dependencies": {},
  "devDependencies": {
    "@jest/globals": "^29.7",
    "@swc/core": "^1.4.17",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.8",
    "jest": "^29.7",
    "typescript": "^5.4",
    "tsup": "^8.0.2"
  }
}