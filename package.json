{
  "name": "tsup-example",
  "type": "commonjs",
  "version": "0.0.0",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    }
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch --onSuccess \"node dist/index.js -e development\"",
    "start": "node dist/index.js -e production",
    "build": "tsup ./src/index.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.12.8",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
